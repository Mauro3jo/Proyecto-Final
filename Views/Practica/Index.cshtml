@model IEnumerable<Turnos.Models.Practica>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create" class="btn-floating btn-large green darken-1"><i class="material-icons">person_add</i></a>
</p>
<table class="responsive-table highlight z-depth-3 blue lighten-5">
    <thead class="orange white-text">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombrePractica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Codigo)
            </th>
           
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombrePractica)
            </td>
             <td>
                @Html.DisplayFor(modelItem => item.Codigo)
            </td>
           
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPractica"><i class="material-icons">edit</i></a> 
                
                <a asp-action="Delete" asp-route-id="@item.IdPractica"><i class="material-icons red-text">delete</i></a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
